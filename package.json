{
  "name": "getting-crypto",
  "version": "0.4.0",
  "author": "Max Schweigerdt",
  "description": "prototype  MEAN Stack with TypeScript",
  "keywords": [
    "MongoDB",
    "Express",
    "Angular 5",
    "NodeJS",
    "TypeScript",
    "Cryopto currency"
  ],
  "repository": {
    "type": "git",
