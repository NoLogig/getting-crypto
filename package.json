  "license": "MIT",
  "engines": {
    "node": "9.2.0",
    "npm": "5.5.1"
  },
  "main": "server/bin/www.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build:C": "ng build -prod -env=prod -sm=false -aot=true",
    "build:S": "tsc -p ./server",
    "build": "npm run build:C && npm run build:S",
    "server:run": "npm run build:S && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build"
  },
  "private": true,
